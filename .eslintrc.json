{
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    "require-jsdoc": "off",
    "linebreak-style": 0,
    "react/react-in-jsx-scope": "off",
    "object-curly-spacing": "off",
    "no-unused-vars": "warn",
    "no-trailing-spaces": "warn"
  },
  "extends": ["google", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"]
}
