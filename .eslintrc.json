{
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    "require-jsdoc": "off",
    "linebreak-style": 0,
    "react/react-in-jsx-scope": "off",
    "object-curly-spacing": "off",
    "no-unused-vars": "warn",
    "no-trailing-spaces": "warn",
    "react/prop-types": "off",
    "operator-linebreak": "off",
    "parser": "babel-parser",
    "indent": "off"
  },
  "extends": [
    "google",
    "plugin:react/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"]
}
